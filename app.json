{
  "expo": {
    "name": "react-spotify",
    "slug": "react-spotify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "reactspotify",
    "userInterfaceStyle": "automatic",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fac60647-4dac-4c45-b355-c347f4a67da9"
      }
    },
    "android": {
      "package": "com.cassiorodp.reactspotify"
    }
  }
}
